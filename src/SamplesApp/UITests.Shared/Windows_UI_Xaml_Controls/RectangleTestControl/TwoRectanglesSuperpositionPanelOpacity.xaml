<UserControl x:Class="UITests.Shared.Windows_UI_Xaml_Controls.RectangleTestControl.TwoRectanglesSuperpositionPanelOpacity"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:local="using:UITests.Shared.Windows_UI_Xaml_Controls.RectangleTestControl"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:controls="using:Uno.UI.Samples.Controls"
			 mc:Ignorable="">

	<controls:SampleControl SampleDescription="Two_Rectangles_In_SuperpositionPanel_WithOpacity">
		<controls:SampleControl.SampleContent>
			<DataTemplate>
				<Grid  Background="HotPink">

					<Rectangle Fill="Red"
							   Width="200"
							   Height="75" />

					<Rectangle Fill="Blue"
							   Opacity="0.5"
							   Width="75"
							   Height="200" />
				</Grid>
			</DataTemplate>
		</controls:SampleControl.SampleContent>
	</controls:SampleControl>
</UserControl>
