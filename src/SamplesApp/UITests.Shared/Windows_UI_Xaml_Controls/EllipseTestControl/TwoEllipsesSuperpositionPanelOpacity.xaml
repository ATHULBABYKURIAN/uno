<UserControl x:Class="UITests.Shared.Windows_UI_Xaml_Controls.EllipseTestControl.TwoEllipsesSuperpositionPanelOpacity"
			 xmlns:controls="using:Uno.UI.Samples.Controls"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:local="using:UITests.Shared.Windows_UI_Xaml_Controls.EllipseTestControl"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 mc:Ignorable="">

	<controls:SampleControl SampleDescription="Two_Ellipses_In_SuperpositionPanel_WithOpacity">
		<controls:SampleControl.SampleContent>
			<DataTemplate>
				<Grid>
					
					<Ellipse Fill="Red"
							 Width="200"
							 Height="75" />

					<Ellipse Fill="Blue"
							 Opacity="0.5"
							 Width="75"
							 Height="200" />
				</Grid>
			</DataTemplate>
		</controls:SampleControl.SampleContent>
	</controls:SampleControl>
</UserControl>
