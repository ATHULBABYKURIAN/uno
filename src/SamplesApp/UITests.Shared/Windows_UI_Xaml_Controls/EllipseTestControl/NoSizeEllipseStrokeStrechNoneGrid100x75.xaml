<UserControl x:Class="UITests.Shared.Windows_UI_Xaml_Controls.EllipseTestControl.NoSizeEllipseStrokeStrechNoneGrid100x75"
			 xmlns:controls="using:Uno.UI.Samples.Controls"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:local="using:UITests.Shared.Windows_UI_Xaml_Controls.EllipseTestControl"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 mc:Ignorable="">

	<controls:SampleControl SampleDescription="No_Size_Ellipse_WithStroke_StrechUniform_In_Grid100x75">
		<controls:SampleControl.SampleContent>
			<DataTemplate>
				<Grid Height="75"
					  Width="100"
					  HorizontalAlignment="Stretch"
					  VerticalAlignment="Stretch"
					  Background="Blue">
					<Ellipse Fill="Purple"
							 Stroke="DeepPink"
							 StrokeThickness="5"
							 Stretch="None" />
				</Grid>

			</DataTemplate>
		</controls:SampleControl.SampleContent>
	</controls:SampleControl>
</UserControl>
